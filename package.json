{
  "name": "@zhaow-de/puppeteer-cluster",
  "version": "0.28.0",
  "description": "Cluster management for puppeteer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --collectCoverage --detectOpenHandles  --errorOnDeprecated --logHeapUsage --forceExit --runInBand",
    "dev": "tsc --watch",
    "lint": "tslint -c tslint.json --project test/_config/tsconfig.json \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "author": "Thomas Dondorf",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhaow-de/puppeteer-cluster.git"
  },
  "homepage": "https://github.com/zhaow-de/puppeteer-cluster",
  "keywords": [
    "puppeteer",
    "cluster",
    "pool"
  ],
  "license": "MIT",
  "dependencies": {
    "debug": "^4.3.3"
  },
  "peerDependencies": {
    "puppeteer": "^12.0.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "coveralls": "^3.1.1",
    "express": "^4.17.1",
    "jest": "^27.4.3",
    "puppeteer": "^12.0.1",
    "puppeteer-core": "^12.0.1",
    "tree-kill": "^1.2.2",
    "ts-jest": "^27.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 7"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/_config/setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/test/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
